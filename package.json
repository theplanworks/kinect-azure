{
	"name": "kinect-azure",
	"version": "0.0.20",
	"description": "Nodejs library to access the azure kinect data from the official SDK",
	"repository": "https://github.com/wouterverweirder/kinect-azure",
	"main": "lib/kinect-azure.js",
	"dependencies": {
		"bindings": "~1.2.1",
		"download": "^7.1.0",
		"extract-zip": "^1.7.0",
		"fs-extra": "^8.1.0",
		"node-addon-api": "^1.0.0"
	},
	"engines": {
		"node": ">= 10.6.0"
	},
	"scripts": {
		"install": "node ./scripts/clean.js",
		"install-windows": "node ./scripts/install.js && node-gyp rebuild",
		"clean": "node ./scripts/clean.js",
		"build": "npm i && cd ./examples/electron && npm start",
		"start": "cd ./examples/electron && npm start",
		"reinstall": "npm run clean && npm run install-windows",
		"prepublishOnly": "rimraf ./examples/**/node_modules"
	},
	"gypfile": true,
	"devDependencies": {
		"rimraf": "^3.0.0"
	}
}
